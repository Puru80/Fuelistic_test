<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fuelistic_test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginSignup.VerifyOTP"></activity>
        <activity android:name=".LoginSignup.SignUp3rdScreen" />
        <activity android:name=".LoginSignup.SignUp2ndScreen" />
        <activity android:name=".LoginSignup.SignUp" />
        <activity android:name=".LoginSignup.AppStartupScreen" />
        <activity android:name=".LoginSignup.Onboarding" />
        <activity android:name=".LoginSignup.Login" />
        <activity android:name=".LoginSignup.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>